{
  "name": "neko",
  "version": "0.0.0",
  "description": "neKo is an innovative chat created by two students from France. It has been designed to offer several brand new user-friendly features.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shenrak/neKo-Api.git"
  },
  "author": "Giom, Veuch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Shenrak/neKo-Api/issues"
  },
  "homepage": "https://github.com/Shenrak/neKo-Api#readme",
  "private": true,
  "dependencies": {
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "superagent": "^3.5.2",
    "webpack": "^2.3.1",
    "wepack": "0.0.1"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.10.0",
    "husky": "^0.13.1",
    "react-scripts": "0.9.4",
    "xo": "^0.17.1"
  },
  "scripts": {
    "start": "start ./public/index.html && webpack -d --watch",
    "build": "webpack -d",
    "test": "xo && react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prepush": " npm run lint --silent",
    "lint": "xo"
  },
  "xo": {
    "envs": [
      "browser"
    ],
    "extends": "xo-react",
    "semicolon": false,
    "space": true,
    "rules": {
      "unicorn/filename-case": "off",
      "import/no-unassigned-import": "off",
      "no-var": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ]
    },
    "overrides": [
      {
        "files": "src/**/*.test.js",
        "envs": [
          "node",
          "browser"
        ],
        "globals": [
          "it"
        ]
      }
    ]
  }
}
